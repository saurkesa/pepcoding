import java.util.Scanner;
/*
 * input : 5
 * output
5
4
3
2
1
 */
public class PrintDecreasing {

	public static void main(String[] args) {

		Scanner s1 = new Scanner(System.in);
		int n = s1.nextInt();
		printDecreasing(n);

	}

	private static void printDecreasing(int n) {

		if (n == 0) {
			return;
		}
		System.out.println(n);
		printDecreasing(n - 1);

	}

}

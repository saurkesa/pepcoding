import java.util.Scanner;
/* output
 * * * * * 
* * * * 
* * * 
* * 
*
*/

public class Pattern2 {

	public static void main(String[] args) {

		Scanner s1 = new Scanner(System.in);
		int n = s1.nextInt();

		for (int i = n; i >= 1; i--) {
			for (int j = 1; j <= i; j++) {
				System.out.print("* ");
			}
			System.out.println();
		}

	}

}

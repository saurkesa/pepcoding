import java.util.Scanner;
/* output
 
 4 * 1 = 4
4 * 2 = 8
4 * 3 = 12
4 * 4 = 16
4 * 5 = 20
4 * 6 = 24
4 * 7 = 28
4 * 8 = 32
4 * 9 = 36
4 * 10 = 40
 */
public class Pattern14 {

	public static void main(String[] args) {

		Scanner s1 = new Scanner(System.in);
		int n = s1.nextInt();

		for (int i = 1; i <= 10; i++) {
			int store = n * i;

			System.out.println(n + " * " + i + " = " + store);

		}

	}

}
